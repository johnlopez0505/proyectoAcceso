extends layout

block content
 h1 Asignaturas Asignadas
 h2 Profesor: #{profesor.nombre} #{profesor.apellido}
 p Email: #{profesor.email}

 if asignaturasAsignadas.length > 0
    ul
      each asignatura in asignaturasAsignadas
        li= asignatura.asignatura
 else
    p El Profesor no está Asignado a ninguna asignatura.

 a(href="/asignar-profesores") Volver atrás